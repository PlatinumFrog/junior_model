            <geometry>
                <mesh filename="file://$(find junior_model)/meshes/robitWheel.stl"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_model)/meshes/robitWheel.stl"/>
            </geometry>
        </collision>
        <inertial>
