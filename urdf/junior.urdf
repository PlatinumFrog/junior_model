            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
    </link>


    <!-- Front Lidar Link -->
    <link name="FrontLidar">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_model)/meshes/robitFrontLidarCase.stl"/>
            </geometry>
            <material name="skyblue"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_model)/meshes/robitFrontLidarCase.stl"/>
