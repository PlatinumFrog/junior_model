<robot name="Junior">
    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>
    
    <material name="red">
        <color rgba="1 0 0 1"/>
    </material>

    <material name="skyblue">
        <color rgba="0.0 0.75 1.0 1.0"/>
    </material>

    <!-- Base Link -->
    <link name="base_link"/>

    <!-- Chassis Link -->
    <link name="Chassis">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_model)/meshes/robitChassis.stl"/>
            </geometry>
            <material name="red"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_model)/meshes/robitChassis.stl"/>
            </geometry>
        </collision>
       <inertial>
            <mass value="0.01"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
    </link>

    <!-- Front Bumper Link -->
    <link name="FrontBumper">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_model)/meshes/robitFrontBumper.stl"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_model)/meshes/robitFrontBumper.stl"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.01"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
    </link>


    <!-- Back Bumper Link -->
