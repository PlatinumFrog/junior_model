            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
    </link>

    <!-- Right Front Link -->
    <link name="WheelRightFront">
        <visual>
            <geometry>
                <mesh filename="file://$(find junior_model)/meshes/robitWheel.stl"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="file://$(find junior_model)/meshes/robitWheel.stl"/>
            </geometry>
        </collision>
       <inertial>
            <mass value="0.01"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
